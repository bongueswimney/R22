# Nemo Application Configuration
# Copy this to .env and configure as needed

# Optional: Google Gemini AI API Key for cloud fallback
# Get your key from: https://makersuite.google.com/app/apikey
NEXT_PUBLIC_GEMINI_API_KEY=

# Development settings (for developers only)
NODE_ENV=production
NEXT_PUBLIC_APP_VERSION=1.0.0

# Backend configuration (auto-configured in production)
BACKEND_PORT=8001
FRONTEND_PORT=3000

# Database configuration (auto-managed)
DATABASE_PATH=./data/nemo.db

# AI Model settings
OLLAMA_HOST=127.0.0.1
OLLAMA_PORT=11434

# Logging level (development only)
LOG_LEVEL=info

# Performance settings
MAX_DATASET_SIZE_MB=100
PYTHON_TIMEOUT_SECONDS=30